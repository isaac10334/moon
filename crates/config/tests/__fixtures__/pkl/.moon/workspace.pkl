codeowners {
	globalPaths = new Mapping {
		["*"] = List("@admins")
	}
	orderBy = "project-name"
	requiredApprovals = 1
	syncOnRun = true
}

constraints {
	enforceProjectTypeRelationships = true
	tagRelationships = Map("a", List("b", "c"))
}

vcs {
	remoteCandidates = new Listing {
		"main"
		"origin/main"
	}
}
