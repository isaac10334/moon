codeowners {
	globalPaths = new Mapping {
		["*"] = List("@admins")
	}
	orderBy = "project-name"
	requiredApprovals = 1
	syncOnRun = true
}

constraints {
	enforceProjectTypeRelationships = false
	tagRelationships = Map("a", List("b", "c"))
}

docker {
	prune {
		deleteVendorDirectories = false
		installToolchainDeps = false
	}
	scaffold {
		copyToolchainFiles = false
		include = List("*.js")
	}
}

generator {
	templates = new Listing {
		"/shared-templates"
		"./templates"
	}
}

vcs {
	remoteCandidates = new Listing {
		"main"
		"origin/main"
	}
}
